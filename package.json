  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "^1.3.1"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.23.1",
  "dependencies": {
    "eslint": "8.19.0",
    "next": "12.2.2"
  }
}
