{
  "name": "bent",
  "description": "Browser Entities. Framework for creating graph-based structures.",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "author": "Matt Styles",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
